# Auto detect text files and perform LF normalization
* text=auto
app.py filter=lfs diff=lfs merge=lfs -text
requirements.txt filter=lfs diff=lfs merge=lfs -text
static filter=lfs diff=lfs merge=lfs -text
templates filter=lfs diff=lfs merge=lfs -text
utils filter=lfs diff=lfs merge=lfs -text
LOCAL_GUI.py filter=lfs diff=lfs merge=lfs -text
README.md filter=lfs diff=lfs merge=lfs -text
